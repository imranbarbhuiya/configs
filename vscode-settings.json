{
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"]
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "cSpell.showAutocompleteSuggestions": true,
  "cSpell.userWords": [
    "Barbhuiya",
    "Hussain",
    "Imran",
    "imranbarbhuiya",
    "keyspace",
    "lngs",
    "mehrin",
    "Middlewares",
    "nextjs",
    "nocheck",
    "nodownload",
    "onhover",
    "parbez",
    "paren",
    "parens",
    "Pentester",
    "preconnect",
    "Webdeveloper"
  ],
  "diffEditor.diffAlgorithm": "advanced",
  "diffEditor.experimental.showMoves": true,
  "discord.detailsEditing": "Editing {file_name} | {file_size}",
  "discord.largeImage": "Editing a {lang} file",
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultColorDecorators": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'JetBrains Mono', 'FiraCode Nerd Font', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.enabled": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.autohide": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 2,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.suggestSelection": "first",
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "markdown": "html",
    "vue-html": "html"
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts, ${capture}.mjs, ${capture}.global.js, ${capture}.test.js",
    "*.jsx": "${capture}.js",
    "*.mjs": "${capture}.mjs.map, ${capture}.min.mjs, ${capture}.d.mts",
    "*.ts": "${capture}.test.ts, ${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.test.tsx, ${capture}.test.ts, ${capture}.module.css, ${capture}.css",
    ".env.local": ".env.*",
    ".eslintrc.json": ".eslint*, .prettier*",
    ".gitignore": ".gitattributes, .gitmodules",
    "jest.config.cjs": "jest-*.cjs, jest.*.cjs",
    "jest.config.js": "jest-*.js, jest.*.js",
    "jest.config.mjs": "jest-*.mjs, jest.*.mjs",
    "package.json": "package-lock.json, yarn.lock, .yarnrc.yml, turbo.json",
    "tsconfig.base.json": "tsconfig.*.json",
    "tsconfig.json": "tsconfig.*.json",
    "vitest.config.ts": "vitest-*.ts, vitest.*.ts"
  },
  "files.associations": {
    "*.html": "html"
  },
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/node_modules/*/**": true,
    "**/target/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.advanced": {
    "slashCommands": {
      "createNotebook": true,
      "createWorkspace": true,
      "search": true
    }
  },
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": true
  },
  "githubPullRequests.pullBranch": "never",
  "gitlens.defaultDateFormat": null,
  "gitlens.views.branches.branches.layout": "list",
  "html.autoClosingTags": true,
  "inlineChat.mode": "live",
  "javascript.autoClosingTags": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": true,
  "remoteHub.commitDirectlyWarning": "off",
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.check.extraArgs": ["--all", "--", "-D", "warnings"],
  "search.exclude": {
    "**/*.code-search": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/yarn.lock": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.autoReplies": {
    "Terminate batch job (Y/N)": "Y\r"
  },
  "terminal.integrated.shellIntegration.enabled": true,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "SECURITY",
    "PRODUCTION"
  ],
  "todo-tree.highlights.customHighlight": {
    "FIXME": {
      "background": "#d800f0",
      "foreground": "#000000"
    },
    "PRODUCTION": {
      "background": "#00ff00",
      "foreground": "#000000",
      "icon": "check",
      "iconColor": "green"
    },
    "SECURITY": {
      "background": "#ff0000",
      "foreground": "#000000",
      "icon": "alert",
      "iconColor": "red"
    },
    "TODO": {
      "background": "#ffd700",
      "foreground": "#000000"
    }
  },
  "typescript.autoClosingTags": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.commandCenter": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#000000"
  },
  "workbench.colorTheme": "Pink Candy Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "top",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "git.ignoreRebaseWarning": true,
  "mdx.validate.validateFileLinks": "ignore",
  "mdx.validate.validateFragmentLinks": "ignore",
  "mdx.validate.validateReferences": "ignore",
  "biome.requireConfiguration": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.hideUnchangedRegions.enabled": true
}
